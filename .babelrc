{
  "presets": ["next/babel", "env", "stage-0"],
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "lib": "./lib",
          "styles": "./styles",
          "components": "./components"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["\\.css$", "\\.less$"]
      }
    ]
  ]
}
